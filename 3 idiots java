import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x1,y1,x2,y2;
        x1=sc.nextInt();
        x2=sc.nextInt();
        y1=sc.nextInt();
        y2=sc.nextInt();
        float a=((float)(x1+y1)/(float)(2.0));
        float b=((float)(x2+y2)/(float)(2.0));
        System.out.println("Binoy's house is located at ("+a+","+b+")");
    }
}
