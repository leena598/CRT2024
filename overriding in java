class Father
{
void surname()
{
System.out.println("k");
}
void designation()
{
System.out.println("clerk");
}
}
class Child extends Father
{
void getName()
{
System.out.println("leena");
}
}
class Testing
{
public static void main(String args[])
{
  Child c1=new Child();
c1.getName();
c1.surname();
c1.designation();
}
}
